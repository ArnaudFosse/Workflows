<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ObjectDefinition>  <information>
    <dynamic>false</dynamic>
    <icon>/images/eclipseIcons/page_obj.gif</icon>
    <configType>xml</configType>
    <visible>true</visible>
    <importIfMandatoryPresent>false</importIfMandatoryPresent>
    <sortvariable>params._order</sortvariable>
    <sortascending>true</sortascending>
    <sortnumerical>true</sortnumerical>
    <sortauto>true</sortauto>
    <order>10</order>
    <description/>
    <name>Policies by Vsys</name>
    <defaultDisplay>false</defaultDisplay>
    <group>Admin|Firewall</group>
  </information>  <variables frozen="0">
    <variable numberOfRowMax="3000" displayName="Vsys Name" displayOrder="0" maxLength="150" name="params.object_id" type="String"/>
    <variable numberOfRowMax="3000" displayName="Policy Name" displayOrder="1" maxLength="150" name="params.policy.0.policy_name" type="String"/>
    <variable numberOfRowMax="3000" displayName="zone_from" displayOrder="2" maxLength="150" name="params.policy.0.zone_from" type="String"/>
    <variable numberOfRowMax="3000" displayName="zone_to" displayOrder="3" maxLength="150" name="params.policy.0.zone_to" type="String"/>
  </variables>  <example>
    <content>
</content>
  </example>
  <command name="IMPORT">
    <operation>type=config&amp;action=show</operation>
    <xpath>/config/devices/entry[@name='localhost.localdomain']/vsys</xpath>
    <parser>
      <section>
        <xpath>//vsys/entry</xpath>
      </section>
      <lines>
        <line>
          <xpath>//entry/@name</xpath>
          <regexp>@name="(?&lt;object_id&gt;.*)"@</regexp>
        </line>
        <line>
          <array name="policy">
            <xpath>//entry/rulebase/security/rules/entry</xpath>
            <lines>
              <line>
                <xpath>//entry/@name</xpath>
                <regexp>@name="(?&lt;policy_name&gt;.*)"@</regexp>
              </line>
              <line>
                <xpath>//entry/from/member</xpath>
                <regexp>@&lt;member&gt;(?&lt;zone_from&gt;.*)&lt;/member&gt;@</regexp>
              </line>
              <line>
                <xpath>//entry/to/member</xpath>
                <regexp>@&lt;member&gt;(?&lt;zone_to&gt;.*)&lt;/member&gt;@</regexp>
              </line>
            </lines>
          </array>
        </line>
      </lines>
    </parser>
  </command>
<command name="UPDATE">
    <rest>type=config&amp;action=edit</rest>
    <xpath>/config/devices/entry[@name='localhost.localdomain']/vsys[@name='{$params.object_id}']/rulebase/security/rules</xpath>
    <operation>{for $params.policy as $policy}
&lt;rules&gt;
    &lt;entry name="{$policy.policy_name}"&gt;
        &lt;option&gt;
            &lt;disable-server-response-inspection&gt;no&lt;/disable-server-response-inspection&gt;
        &lt;/option&gt;
        &lt;from&gt;
            &lt;member&gt;{$policy.zone_from}&lt;/member&gt;
        &lt;/from&gt;
        &lt;to&gt;
            &lt;member&gt;{$policy.zone_to}&lt;/member&gt;
        &lt;/to&gt;
        &lt;source&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/source&gt;
        &lt;destination&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/destination&gt;
        &lt;source-user&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/source-user&gt;
        &lt;category&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/category&gt;
        &lt;application&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/application&gt;
        &lt;service&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/service&gt;
        &lt;hip-profiles&gt;
            &lt;member&gt;any&lt;/member&gt;
        &lt;/hip-profiles&gt;
        &lt;log-start&gt;no&lt;/log-start&gt;
        &lt;log-end&gt;yes&lt;/log-end&gt;
        &lt;log-setting&gt;SOC_Default_Logging&lt;/log-setting&gt;
        &lt;negate-source&gt;no&lt;/negate-source&gt;
        &lt;negate-destination&gt;no&lt;/negate-destination&gt;
        &lt;action&gt;allow&lt;/action&gt;
    &lt;/entry&gt;
&lt;/rules&gt;
{/for}</operation>
  </command>
</ObjectDefinition>
